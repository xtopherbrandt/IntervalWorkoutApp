using Toybox.WatchUi as Ui;
using Toybox.Graphics as Gfx;
using Toybox.Timer as Timer;

class StepView extends Ui.View 
{
	hidden var _workout;
	
	function initialize ( workout )
	{
		_workout = workout;
	}
	
    function onLayout(dc)
    {
    	
     	setLayout( Rez.Layouts.IntervalStepLayout( dc ) );
        onUpdate( dc );
    }
	
	
    function onUpdate(dc)
    {
    	var currentInterval;
    	var nextInterval;
    	var activity;
    	
    	currentInterval = _workout.getCurrentInterval(); 	
    	nextInterval = _workout.getNextInterval();
        
        if ( currentInterval.intervalType == STEP_LAP )
        {
        	lapStepView( dc, currentInterval );
        }
        else if ( currentInterval.intervalType == STEP_TIME )
        {
        	timeStepView( dc, currentInterval );
        }
        else if ( currentInterval.intervalType == STEP_DISTANCE )
        {
        	distanceStepView( dc, currentInterval );
        }
        else if ( currentInterval.intervalType == SET_ON_TIME )
        {
        	onTimeRestStepView( dc, currentInterval );
        }
        else if ( currentInterval.intervalType == SET_ON_DISTANCE )
        {
        	onDistanceRestStepView( dc, currentInterval );
        }
        else if ( currentInterval.intervalType == WORKOUT )
        {
        	completeStepView( dc, currentInterval );
        }
        else
        {
        }
        
        if ( nextInterval == null )
        {
        	clearNextStepView( dc );
        }
        else if ( nextInterval.intervalType == STEP_LAP )
        {
        	lapNextStepView( dc, nextInterval );
        }
        else if ( nextInterval.intervalType == STEP_TIME )
        {
        	timeNextStepView( dc, nextInterval );
        }
        else if ( nextInterval.intervalType == STEP_DISTANCE )
        {
        	distanceNextStepView( dc, nextInterval );
        }
        else if ( nextInterval.intervalType == SET_ON_TIME )
        {
        	onTimeRestNextStepView( dc, nextInterval );
        }
        else if ( nextInterval.intervalType == SET_ON_DISTANCE )
        {
        	onDistanceRestNextStepView( dc, nextInterval );
        }
        else if ( nextInterval.intervalType == WORKOUT )
        {
        	completeNextStepView( dc, nextInterval );
        }
        else
        {
        	
        }

        View.onUpdate(dc);
        
        return true;
        
    }

	function setField ( dc, drawableId, text )
	{
        var drawable;
        
        //set the interval name
        drawable = View.findDrawableById( drawableId );
        drawable.setText( text );
        drawable.draw(dc);
	}
	
	function setCurrentStep ( dc, intervalName, intervalUntilText, intervalLength, intervalRepeatStep, intervalRepeatDivider, intervalRepeatCount )
	{
        
        //set the interval name
		setField( dc, "IntervalName", intervalName );
        
        //set the until / for
		setField( dc, "Until", intervalUntilText );
        
        //set the interval length
		setField( dc, "IntervalLength", intervalLength );
        
        //set the interval repeat step
		setField( dc, "IntervalRepeatStep", intervalRepeatStep );
        
        //set the interval repeat divider
		setField( dc, "IntervalRepeatDivider", intervalRepeatDivider );
        
        //set the interval repeat count
		setField( dc, "IntervalRepeatCount", intervalRepeatCount );
	}
	
	function setNextStep ( dc, intervalName, intervalUntilText, intervalLength, intervalRepeatStep, intervalRepeatDivider, intervalRepeatCount )
