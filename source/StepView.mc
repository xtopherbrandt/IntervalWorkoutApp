        //set the interval repeat divider
		setField( dc, "NextIntervalRepeatDivider", intervalRepeatDivider );
        
        //set the interval repeat count
		setField( dc, "NextIntervalRepeatCount", intervalRepeatCount );
	}
	
	function lapStepView ( dc, currentInterval )
	{
    	var intervalName = currentInterval.name;
    	var intervalUntilText = "Until";
    	var intervalLength = "Lap Button";
    	var intervalRepeatStep;
    	var intervalRepeatDivider;
    	var intervalRepeatCount;
    	
    	if ( currentInterval.repeats.getStepId() > 0 )
    	{
    		intervalRepeatStep = currentInterval.repeats.getStepId().toString();
    		intervalRepeatDivider = "/";
    		intervalRepeatCount = currentInterval.repeats.getCount().toString();
    	}
    	else
    	{
    		intervalRepeatStep = "";
    		intervalRepeatDivider = "";
    		intervalRepeatCount = "";
    	}

		setCurrentStep( dc, intervalName, intervalUntilText, intervalLength, intervalRepeatStep, intervalRepeatDivider, intervalRepeatCount );
	}

